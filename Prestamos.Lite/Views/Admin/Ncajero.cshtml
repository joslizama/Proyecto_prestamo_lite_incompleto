    <form method="post" enctype="multipart/form-data" id="validar_usuarios">

        <table align="center" class="border">

            <tr>
                <td colspan="4" align="center"><center><h6>Nuevo cajero</h6></center></td>
            </tr>
            <tr>
                <td><label for="formGroupExampleInput">Rut : </label></td>
                <td><input type="text" class="form-control" id="rut" name="rut" required></td>
                <td><label for="formGroupExampleInput">Fecha de nacimiento : </label></td>
                <td colspan="2" align="center"><input type="date" class="form-control" id="fecha" name="fecha" required></td>




            </tr>
            <tr>
                <td><label for="formGroupExampleInput">Nombre : </label></td>
                <td><input type="text" class="form-control" id="nombre" name="nombre" required></td>
                <td><label for="formGroupExampleInput">Apellido : </label></td>
                <td colspan="2" align="center"><input type="text" class="form-control" id="apellido" name="apellido" required></td>




            </tr>
            <tr>
                <td><label for="formGroupExampleInput">Comuna : </label></td>
                <td><input type="text" class="form-control" id="comuna" name="comuna" required></td>

                <td><label for="formGroupExampleInput">Direccion : </label></td>
                <td colspan="2" align="center"><textarea class="form-control" id="direccion" name="direccion" rows="3"></textarea></td>

            </tr>
            <tr>
                <td><label for="formGroupExampleInput">Ciudad : </label></td>
                <td><input type="text" class="form-control" id="ciudad" name="ciudad" required></td>

                <td><label for="formGroupExampleInput">Correo : </label></td>
                <td colspan="2" align="center"><input type="email" class="form-control" id="correo" name="correo" required></td>





            </tr>
            <tr>
                <td><label for="formGroupExampleInput">Telefono : </label></td>
                <td><input type="text" class="form-control" id="telefono" name="telefono" required></td>
                <td><label for="formGroupExampleInput">Contraseña : </label></td>
                <td colspan="2" align="center"><input type="password" class="form-control" id="contraseña" name="contraseña" required></td>

            </tr>



            <tr>
                <td colspan="4" align="center">

                    <button type="submit" name="Nuevo" id="Nuevo" value="Nuevo" class="btn btn-primary">Nuevo</button>
                    &nbsp;
                    <button type="reset" name="Cancelar" id="Cancelar" value="Cancelar" class="btn btn-danger">Cancelar</button>

                </td>


            </tr>



        </table>




    </form>
















